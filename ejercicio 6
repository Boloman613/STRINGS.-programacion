#include <iostream>
#include <string>
using namespace std;
string mezclarCadenas(string c1, string c2) {
    return c1 + c2;  
}

int main() {
    string cadena1, cadena2;

    cout << "Introduce la primera cadena: ";
    cin >> cadena1;

    cout << "Introduce la segunda cadena: ";
    cin >> cadena2;

    string mezcla = mezclarCadenas(cadena1, cadena2);

    cout << "Cadena mezclada: " << mezcla << endl;

    return 0;
}
